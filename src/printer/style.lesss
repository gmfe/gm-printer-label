html,
body {
  font-size: 14px;
}

.gm-printer-label {
  background: white;
  page-break-before: always
}

.gm-printer-label-page {
  overflow: hidden;
  position: relative;
}

// block
.gm-printer-label-block {
  position: relative;

  .gm-printer-label-block-text-edit {
    display: none;
  }
}

// 预分拣，马来整体标签大小样式
.gm-printer-pre-sort-malai{
  width: calc(7cm - .2cm);
  height: calc(5cm - .3cm);
  overflow: hidden;
  margin: .3cm 0 0 .2cm;
}

@page {
  margin: 0;
}
